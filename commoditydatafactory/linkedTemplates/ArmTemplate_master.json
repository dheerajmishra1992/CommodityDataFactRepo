{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "commoditydatafactory"
		},
		"AzureDataLakeStore_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataLakeStore'"
		},
		"AzureDataLakeStore_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://commoditydatalake.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "ee149afd-9918-418e-a2af-4eedf935b250"
		},
		"AzureDataLakeStore_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "842b637b-935b-4a2b-99aa-559fe5024c67"
		},
		"AzureDataLakeStore_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "bee17de5-4061-4f81-906c-75e2a3209662"
		},
		"AzureDataLakeStore_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "rs-training"
		},
		"commodityapils_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.data.gov.in/resource/9ef84268-d588-465a-a308-a864a43d0070?api-key=579b464db66ec23bdd00000147c9b85db8cc419a6de5754298ed72a1&format=csv&offset=0&limit=10000"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "commoditydatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStore_servicePrincipalKey": {
						"value": "[parameters('AzureDataLakeStore_servicePrincipalKey')]"
					},
					"AzureDataLakeStore_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStore_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_resourceGroupName')]"
					},
					"commodityapils_properties_typeProperties_url": {
						"value": "[parameters('commodityapils_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}